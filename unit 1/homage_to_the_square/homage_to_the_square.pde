//shapes, shapes, shapes...
//Homage to the Square: "Ascending"

void setup()
{
  
  background(255);
  size(500, 500);
  noStroke();
  
}

void draw()
{
  
  rectMode(CENTER);
  
  fill(#81D0FF);
  square(250, 250, 490);
  
  fill(#B6B8B9);
  square(250, 280, 390);
  
  fill(255);
  square(250, 300, 290);
  
  fill(#FCF000);
  square(250, 320, 190);
  
}
